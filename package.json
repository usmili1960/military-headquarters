{
  "name": "military-headquarters",
  "version": "1.0.0",
  "description": "United States Military Headquarters - Washington DC",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "startup": "node scripts/startup.js",
    "check-db": "node scripts/check-persistence.js check",
    "clear-db": "node scripts/check-persistence.js clear",
    "serve-5500": "npx http-server src -p 5500 -c-1",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint server/ src/js/",
    "docker:build": "docker build -t military-hq:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f backend",
    "db:migrate": "node scripts/migrate.js"
  },
  "keywords": ["military", "headquarters", "usa"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "nodemailer": "^6.9.1",
    "mongoose": "^7.0.3",
    "jsonwebtoken": "^9.0.2",
    "validator": "^13.11.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "uuid": "^9.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0"
  }
}
